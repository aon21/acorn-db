{
  "name": "tiny-pixel/acorn-models",
  "type": "package",
  "license": "MIT",
  "description": "Eloquent database support for Acorn projects",
  "authors": [
    {
      "name": "Kelly Mears",
      "email": "kelly@tinypixel.dev"
    }
  ],
  "keywords": [
    "wordpress",
    "acorn",
    "sage",
    "eloquent"
  ],
  "support": {
    "issues": "https://github.com/pixelcollective/acorn-models/issues"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\AcornModels\\": "src/"
    },
    "scripts": [
      "src/helpers.php"
    ]
  },
  "require": {
    "php": ">=7.3"
  },
  "extra": {
    "acorn": {
      "providers": ["TinyPixel\\AcornModels\\DatabaseServiceProvider"]
    }
  }
}
