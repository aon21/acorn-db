{
  "name": "tiny-pixel/acorn-database",
  "type": "package",
  "license": "MIT",
  "description": "Eloquent database support for Acorn projects",
  "authors": [
    {
      "name": "Kelly Mears",
      "email": "kelly@tinypixel.dev"
    }
  ],
  "keywords": [
    "wordpress",
    "acorn",
    "sage",
    "eloquent"
  ],
  "support": {
    "issues": "https://github.com/pixelcollective/acorn-database/issues"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\Acorn\\Database\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3",
    "sofa/eloquence": "^5.6",
    "fzaninotto/faker": "^1.8"
  },
  "require-dev": {
    "tiny-pixel/php-docz": "dev-master"
  },
  "extra": {
    "acorn": {
      "providers": [
        "Sofa\\Eloquence\\BaseServiceProvider",
        "TinyPixel\\Acorn\\Database\\Providers\\DatabaseServiceProvider"
      ]
    }
  }
}
